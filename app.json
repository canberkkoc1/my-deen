{
  "expo": {
    "name": "mydeenapp",
    "slug": "my-deen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Tinted.png",
    "scheme": "mydeen",
    "userInterfaceStyle": "automatic",
