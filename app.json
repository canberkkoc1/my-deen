{
  "expo": {
    "name": "my-deen",
    "slug": "my-deen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Tinted.png",
    "scheme": "mydeen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3782077697240059~6068378967"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.dvcanberkkoc.mydeen",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "aps-environment": "development",
        "NSLocationWhenInUseUsageDescription": "We use your location to calculate prayer times and Qibla direction.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We use your location to calculate prayer times and Qibla direction."
      }
    },
    "android": {
      "package": "com.dvcanberkkoc.mydeen",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "dark": {
            "backgroundColor": "#ffffff",
            "image": "./assets/images/splash-icon.png"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "android_app_id": "ca-app-pub-3782077697240059~6068378967",
          "ios_app_id": "ca-app-pub-3782077697240059~6068378967"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(MyDeen) to use your location.",
          "locationWhenInUsePermission": "Allow $(MyDeen) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5fa8ed86-5301-48c7-bcb2-eb9abf58c595"
      }
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}